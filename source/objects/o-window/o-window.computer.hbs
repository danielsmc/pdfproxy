<input
    type="file"
    class="c-window__file-upload" />

<ol class="o-window__files">

    <li class="c-window__file">
        <span class="c-window__link">
            <img
                alt=""
                src="images/components/c-icon/hdd.png" />
            HDD
        </span>
    </li>

    <li class="c-window__file">
        <a
            class="c-window__link c-window__link--printer"
            onclick="window.print();">
